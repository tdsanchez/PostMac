<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.local.apfs-monitor</string>

	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/apfs-monitor</string>
		<string>-daemon</string>
		<string>-interval</string>
		<string>5m</string>
		<string>-warning</string>
		<string>100</string>
		<string>-critical</string>
		<string>50</string>
		<string>-log</string>
		<string>/Users/tdsanchez/Library/Logs/apfs-monitor.log</string>
	</array>

	<key>RunAtLoad</key>
	<true/>

	<key>KeepAlive</key>
	<true/>

	<key>StandardErrorPath</key>
	<string>/Users/tdsanchez/Library/Logs/apfs-monitor.err</string>

	<key>StandardOutPath</key>
	<string>/Users/tdsanchez/Library/Logs/apfs-monitor.out</string>

	<key>WorkingDirectory</key>
	<string>/Users/tdsanchez</string>

	<key>ThrottleInterval</key>
	<integer>300</integer>
</dict>
</plist>
